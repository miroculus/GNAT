<entry key="predictionsOutput">train.set1.txtNoMethods.predictions</entry>

<!-- 'test' mode returns some debug info -->
<entry key="testDirectory">abstracts</entry>
<!-- 'evaluation' mode compares the predictions to a given gold standard -->
<entry key="evalDirectory">data/allSpecies/100</entry>
<entry key="evalSolution">data/allSpecies/test100.genelist</entry>
<entry key="evalScript">data/allSpecies/bc2scoring.py</entry>

